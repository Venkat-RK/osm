<div>
  <form method="post" action="/osm/search">
    <table cellpadding="0" cellspacing="0" width="50%">
     <tr>
        <th align="left">Object Type:</th>
        <th align="left">Object Id:</th>
        <th align="left">From:</th>
        <th align="left">To:</th>
         <th></th>
      </tr>
      <tr>
        <td>
          <select name="object_type">
            <% get_object_types.each do |object_type|%>
              <option value=<%= object_type.to_s %> <%=is_selected(object_type)%>><%= object_type %></option>
            <% end%>
          </select>
        </td>
        <td>
          <input type="text" name="object_id" value= <%= get_object_id_value %>>
        </td>
        <td>
          <input type="text" name="from" value=<%= get_from_value %>>
        </td>
        <td>
          <input type="text" name="to" value=<%= get_to_value %>>
        </td>
        <td>
          <input type='submit' value='Search'></input>
        </td>
      </tr>
    </table>
  </form>
</div>
